package data;

import org.newdawn.slick.opengl.Texture;

public class Tile 
{
	private float x;
	private float y;
	private float width;
	private float height;
	private Texture texture;
	
	public Tile(float x, float y, float width, float height, Texture texture)
	{
		this.x = x;
		this.y = y;
		this.width = width;
		this.height = height;
	}
}
