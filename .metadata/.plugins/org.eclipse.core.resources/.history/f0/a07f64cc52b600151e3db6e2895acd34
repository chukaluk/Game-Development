package UI;

import org.newdawn.slick.opengl.Texture;

public class Button {
	
	private String name;
	private Texture texture;
	private int x, y, width, height;
	
	public Button(String name, Texture texture, int x, int y, int width, int height) {
		this.name = name;
		this.texture = texture;
		this.x = x;
		this.y = y;
		this.width = width;
		this.height = height;
	}
}
